{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/getstats",
			"dest": "/index.js"
		},
		{
			"src": "/api/addstats/:userid/:winorloss",
			"dest": "/index.js"
		},
		{
			"src": "/api/addnewstat/:discordId/:discordUsername",
			"dest": "/index.js"
		},
		{
			"src": "/api/deletestats/:userid",
			"dest": "/index.js"
		}
	]
}
